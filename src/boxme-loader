[ Config ]

shell.name=boxme-loader
shell.args=init,

init=exec if ($BOOT_VM == $.BOOT_VM) x11 quest .boxme-loader & if ($BOOT_VM != $.BOOT_VM) execute tick Starting...; 


[ DISPLAY ]

quest.title=BoxME - Boot a Machine
quest.label=Machine setting file:
quest.cmd=execute boxme-loader init
quest.key=BOOT_VM